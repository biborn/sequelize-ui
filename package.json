{
  "name": "sequelize-ui",
  "version": "0.2.0",
  "description": "GUI for designing relational database schema and exporting as formatted Sequlize template.",
  "author": "Tom Schuster <tomjschuster@gmail.com> (https://github.com/tomjschuster)",
  "license": "ISC",
  "homepage": "http://dev-sequelizeui.herokuapp.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomjschuster/sequelize-ui.git"
  },
  "bugs": {
    "url": "https://github.com/tomjschuster/sequelize-ui/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --config ./webpack.prod.config.js --progress --colors",
    "deploy": "npm run build && git add dist/app.js && git commit -m 'build for deployment' && git push heroku master && git push origin master",
    "start": "NODE_ENV=prod node ./server",
    "build:dev": "NODE_ENV=dev webpack --config ./webpack.dev.config.js --progress --colors",
    "start:dev": "npm run build:dev && NODE_ENV=dev nodemon ./server"
  },
  "engines": {
    "node": "7.10.0"
  },
  "dependencies": {
    "express": "^4.15.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "eslint": "^3.19.0",
    "eslint-config-fullstack": "^3.0.0",
    "eslint-plugin-react": "^7.0.1",
    "nodemon": "^1.11.0",
    "webpack": "^2.6.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
